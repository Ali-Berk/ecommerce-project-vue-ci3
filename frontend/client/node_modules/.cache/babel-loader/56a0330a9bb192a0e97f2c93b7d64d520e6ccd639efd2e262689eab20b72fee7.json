control:invalid {\n  border-color: #dc3545;\n}\n\n.btn-primary {\n  background-color: #6c5ce7;\n  border-color: #6c5ce7;\n}\n\n.btn-primary:hover {\n  background-color: #5a4ddf;\n  border-color: #5a4ddf;\n}\n\n.invalid-feedback1{\n  color: #dc3545;\n}\n</style>\n"],"mappings":";AAgDA,OAAOA,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,KAAK,EAAC,CAAC,UAAU,CAAC;EAClBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,SAAS,EAAE,KAAK;MAChBC,GAAG,EAAE;IACP,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,WAAWA,CAAA,EAAG;MAClB,IAAI,CAACH,SAAQ,GAAI,IAAI;MAErB,IAAI,CAAC,IAAI,CAACF,KAAI,IAAK,CAAC,IAAI,CAACC,QAAQ,EAAE;QACjC,IAAI,CAACE,GAAE,GAAI,IAAI;QACf;MACF;MACA,MAAM;QAACJ;MAAI,IAAI,MAAMH,KAAK,CAACU,IAAI,CAAC,iCAAiC,EAAC;QAChEC,IAAI,EAAE,IAAI,CAACP,KAAK;QAChBC,QAAQ,EAAE,IAAI,CAACA;MACjB,CAAC,EAAE;QAACO,eAAe,EAAC;MAAI,CAAE;MAE1B,IAAGT,IAAI,CAACU,MAAK,KAAM,OAAO,EAAE;QAC1B,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,GAAG,CAAC;QACtB,IAAI,CAACC,KAAK,CAAC,eAAe,EAAEb,IAAI,CAACc,IAAI,CAAC;QACtCC,OAAO,CAACC,GAAG,CAAChB,IAAI,CAACU,MAAM,CAAC;MAC1B,OACI;QACFK,OAAO,CAACC,GAAG,CAAChB,IAAI;QAChB,IAAI,CAACI,GAAE,GAAI,IAAI;MACjB;MACAW,OAAO,CAACC,GAAG,CAAC,kBAAkB,EAAE;QAAEf,KAAK,EAAE,IAAI,CAACA,KAAK;QAAEC,QAAQ,EAAE,IAAI,CAACA;MAAQ,CAAC,CAAC;MAC9Ea,OAAO,CAACC,GAAG,CAAChB,IAAI,CAAC;IACnB;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}